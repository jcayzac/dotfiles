[push]
	default = current
[color]
	ui = auto
[merge]
	tool = vimdiff
[gc]
	auto = 1
[format]
	pretty = fuller
[alias]
	browser = instaweb -d webrick
	backlog = log --graph --pretty=format:'%C(white red bold) %h %Creset %C(cyan)%cn%Creset %s %Cgreen(%cr)%Creset%C(yellow)%d%Creset' --abbrev-commit
	shortlog = !git backlog -n10
